{
    "name": "personalwebpage",
    "version": "1.0.0",
    "description": "Personal Webpage",
    "main": "Index.js",
    "type": "module",
    "scripts": {
        "start": "nodemon ./Src/Index.js",
        "lint": "eslint",
        "test": "jest"
    },
    "author": "Ethan",
    "license": "ISC",
    "devDependencies": {
        "eslint": "^7.5.0",
        "eslint-config-google": "^0.14.0",
        "jest": "^26.1.0"
    },
    "engines": {
        "node": ">=12.0.0",
        "npm": ">=6.0.0"
    },
    "dependencies": {
        "bcrypt": "^5.0.0",
        "cookie-parser": "^1.4.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "node-schedule": "^1.3.2",
        "request": "^2.88.2",
        "validator": "^13.1.1"
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "commonjs": true,
            "es2020": true
        },
        "extends": [
            "google"
        ],
        "parserOptions": {
            "ecmaVersion": 11,
            "sourceType": "module"
        },
        "rules": {
            "no-unused-vars": "warn",
            "linebreak-style": [
                "error",
                "windows"
            ],
            "eol-last": [
                "error",
                "never"
            ],
            "object-curly-spacing": [
                "error",
                "always"
            ],
            "indent": [
                "error",
                4,
                {
                    "SwitchCase": 1
                }
            ],
            "max-len": [
                "error",
                {
                    "code": 200,
                    "ignoreComments": true,
                    "ignoreUrls": true
                }
            ],
            "require-jsdoc": [
                "error",
                {
                    "require": {
                        "FunctionDeclaration": false,
                        "MethodDefinition": false,
                        "ClassDeclaration": false,
                        "ArrowFunctionExpression": false,
                        "FunctionExpression": false
                    }
                }
            ]
        }
    }
}
